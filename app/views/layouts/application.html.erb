<html class="no-js" lang="en-US">
	<!--<![endif]-->

<head>
  <meta charset="utf-8">
		<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<title>Launcherr | Crowdsourcing Creative Projects</title>
		<!-- FONTS -->
		<link href='http://fonts.googleapis.com/css?family=Open+Sans:300,400,800,600|Raleway:300,600,900' rel='stylesheet' type='text/css'>
		<!-- FONTS -->
		 <!-- Bootstrap CSS-->
    <link href="/files/css/bootstrap.min.css" rel="stylesheet" media="screen">
		<!-- CSS -->
<link href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
		<!-- FAVICON -->
		<link rel="shortcut icon" href="files/uploads/favicon.png"/>
		<!-- FAVICON -->

		
  <%= stylesheet_link_tag "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
<!-- Prelang: Analytics / Google Analytics -->
    <%= analytics_init if Rails.env.production? %><meta name="viewport" content="width=device-width, initial-scale=1"/></head>
<body>
<!-- header -->
<%= render partial: "elements/navbar" %>
<%= bootstrap_flash %>
<div id="page-content" class="fixed-header">
<header id="header">
				<div class="header-inner wrapper clearfix">

					<div id="logo" class="left-float">
						<a id="defaut-logo" class="logotype" href="/">
						<%= image_tag("headerlogo.png", :size => "64x64", :alt => "Show Group") %></a>
					</div>

					<div class="menu left-float clearfix">
						<nav id="main-nav">
							</li>

							</ul>
						</nav>
						<nav id="menu-controls">
							
							
						</nav>
					</div>
					<center>
					<div class="right-float" style="text-align: left">
					<!-- Logged in Header-->	
<% if defined?(Devise) %>
        <% if user_signed_in? %>
	<nav id="main-nav">
<ul>

<li>
<a href="#"> Welcome </a>
<ul class="sub-menu">
           <li><%= link_to "My Projects", my_project_posts_path %></li>
</li>

<li><%= link_to "My Account", edit_user_registration_path %></li>
</li>
<% x = 7 %>
<li><%= link_to "Inbox", inbox_posts_path %></li>
</li>
<li><%= current_user.received_messages.count %></li>
<li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
</li>

</ul>
</li>

</ul>
							<!-- END Logged in Header -->
<% else %>
				<div class="right-float" style="text-align: left">
						<nav id="main-nav">
							<ul>
          <li><%= link_to "Login", new_user_session_path %></li>
          <li><%= link_to "Sign Up", new_user_registration_path %></li>
        <% end %>
      <% end %>
						</nav>
					</div>
					</center>

				</div>
				</header>
		</div>
			
                      
	<% if flash[:error] %>
  <p class="error" > <%= flash[:error].html_safe %></p><br />
<%end%>
<% if flash[:alert] %>
  <p class="error" > <%= flash[:alert].html_safe %></p><br />
<%end%>

		


<%= yield %>



<!-- FOOTER -->
				<footer>
				  <div class="footerinner wrapper align-center text-light">
						<a id="backtotop" href="#" class="sr-button sr-buttonicon small-iconbutton"><i class="fa fa-angle-up"></i></a>
						<p class="footer-logo"><%= image_tag("footerlogo.png", :size => "64x64", :alt => "Show Group") %></a>
						</p>
					<ul class="socialmedia-widget social-share">
							<li class="facebook">
								<a href="http://facebook.com/lnchrr" target="_blank">Facebook</a>
							</li>
							<li class="twitter">
								<a href="http://twitter.com/lnchrr" target="_blank">Tweet</a>
							</li>
							<li class="linkedin">
								<a href="http://www.linkedin.com/company/launcherr" target="_blank"> Linkedin</a>
							</li>
							<li class="instagram">
								<a href="http://instagram.com/lnchrr" target="_blank">Instagram</a>
							</li>
				    </ul>
					  
					  <p class="copyright"><a href="http://blog.launcherr.com/about">About</a> | <a href="http://blog.launcherr.com">Blog</a> | <a href="http://blog.launcherr.com/contact">Contact</a></p>
					  </p>
						<p class="copyright">Copyright &copy; 2014 - Launcherr - Crowdsourcing Creative Projects
				    </p>
					</div>
		  </footer>
				<!-- FOOTER -->
			<script>

				$( 'document' ).ready( function( ) {
					//INITIALIZE THE PLUGIN
					$( '#grid' ).grid( );

				} );

			</script>
                

</body>
</html>
